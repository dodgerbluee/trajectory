{
  "name": "trajectory-backend",
  "version": "1.0.0",
  "description": "Backend API for growth tracker",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "db:start": "docker-compose up -d database",
    "db:stop": "docker-compose stop database",
    "db:logs": "docker-compose logs -f database",
    "db:reset": "docker-compose down -v && docker-compose up -d database"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.12",
    "@types/pg": "^8.11.6",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^8.57.0",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5"
  }
}
