# Production Environment Variables
# Copy this to .env and set appropriate values

# Docker Image (defaults to latest, override for specific version)
# IMAGE=ghcr.io/your-org/trajectory:latest
# IMAGE=ghcr.io/your-org/trajectory:dev-20250125-abc1234

# Database Configuration
DB_PASSWORD=CHANGE_THIS_TO_STRONG_PASSWORD
POSTGRES_DB=trajectory
POSTGRES_USER=trajectory_user

# App Port (optional - default: 3001)
# Only needed if exposing directly (not using NPM)
PORT=3001
