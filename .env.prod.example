# Production Environment Variables
# Copy this to .env and set appropriate values

# Database Configuration
DB_PASSWORD=CHANGE_THIS_TO_STRONG_PASSWORD
POSTGRES_DB=trajectory
POSTGRES_USER=trajectory_user

# App Port (optional - default: 3001)
# Only needed if exposing directly (not using NPM)
PORT=3001

# Frontend Configuration
# API URL (leave empty for relative URLs, or set to full API URL)
VITE_API_URL=

# Base path for NPM path routing (e.g., /trajectory)
# Leave empty for subdomain setup or root path
# Set to /trajectory for path-based routing
VITE_BASE_PATH=
